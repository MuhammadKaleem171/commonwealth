{
  "name": "@commonxyz/api-client",
  "version": "0.0.1",
  "type": "module",
  "description": "Common Client Proxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicommonwealth/commonwealth.git"
  },
  "bugs": {
    "url": "https://github.com/hicommonwealth/commonwealth/issues"
  },
  "homepage": "https://github.com/hicommonwealth/commonwealth#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "default": "./build/index.js",
      "devmode": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.build.json",
    "clean": "rm -rf build && rm -rf coverage",
    "publish": "npm publish",
    "run-test": "tsx ./test/index.ts",
    "check-types": "tsc --noEmit",
    "fern-init": "fern init --openapi http://localhost:8080/api/v1/openapi.json --org Common",
    "generate-client-fern": "fern init --openapi http://localhost:8080/api/v1/openapi.json && fern check && fern add fern-typescript-node-sdk --group sdk && fern generate --group sdk"
  },
  "keywords": [
    "commonwealth",
    "dao",
    "community"
  ],
  "author": "common.xyz",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "moment": "^2.23.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/moment": "^2.13.0",
    "tsx": "^4.7.2"
  }
}
